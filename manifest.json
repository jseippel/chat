{
  "manifest_version": 2,

  "name": "Chat App",
  "description": "App for chat",
  "version": "0.1",

  "browser_action": {
    "default_icon": {
        "16": "images/chat-16.png",
        "48": "images/chat-48.png",
        "128": "images/chat-128.png"
    },
    "default_popup": "popup.html",
    "default_title": "Chat"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://www.messenger.com/",
    "*://hangouts.google.com/"
  ]
}
